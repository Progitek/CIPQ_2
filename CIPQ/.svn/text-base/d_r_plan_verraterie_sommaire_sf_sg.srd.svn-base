 retrieve="SELECT 	cipqQryPlanVerraterieSommaireSG.CIE_NO, 
			cipqQryPlanVerraterieSommaireSG.SousGroupe, 
			Count(cipqQryPlanVerraterieSommaireSG.CIE_NO) AS CompteSousGroupe 
FROM (SELECT t_emplacementverrat.CIE_NO, 
				 Left(Sous_Groupe,1) AS SousGroupe, 
				 Left(t_emplacementverrat.Emplacement,1) AS LaSection, 
				 T_Verrat.CodeRACE
		FROM	T_Verrat INNER JOIN t_emplacementverrat ON t_emplacementverrat.tatouage = T_Verrat.tatouage AND 
                                                                                   t_emplacementverrat.dateemplacement = (select max(dateemplacement) 
                                                                                                                          from t_emplacementverrat as verremp 
                                                                                                                          where verremp.tatouage = t_emplacementverrat.tatouage and 
                                                                                                                                :ra_de >= verremp.dateemplacement)
		WHERE T_Verrat.CodeRACE='LO' AND 
				:ra_de between t_Verrat.ENTPRODUCT and isNull(t_Verrat.ELIMIN, :ra_de) AND 
				Not T_Verrat.CodeVerrat Is Null) as cipqQryPlanVerraterieSommaireSG
WHERE cipqQryPlanVerraterieSommaireSG.CIE_NO = :RA_CIENO
GROUP BY cipqQryPlanVerraterieSommaireSG.CIE_NO, 
			cipqQryPlanVerraterieSommaireSG.SousGroupe 
ORDER BY cipqQryPlanVerraterieSommaireSG.CIE_NO, cipqQryPlanVerraterieSommaireSG.SousGroupe;
" arguments=(("RA_CIENO", string),("ra_de", date)) )
