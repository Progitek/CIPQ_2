table(column=(type=char(9) updatewhereclause=yes name=gl dbname="gl" )
 column=(type=char(3) updatewhereclause=yes name=cie dbname="cie" )
 column=(type=char(6) updatewhereclause=yes name=compte dbname="compte" )
 column=(type=char(0) updatewhereclause=yes name=cc_date_periode dbname="cc_date_periode" )
 column=(type=number updatewhereclause=yes name=total dbname="total" )
 retrieve="SELECT DISTINCT
CComptant + '-' + DComptant AS GL,
If IsNull(t_Transport.NoClasse,t_produit.NoClasse) = 19 And actif = 0 then
	IsNull(t_verrat.CIE_NO,#temp_acc_som_statfactureDetail.CIE_NO)
ELSE
	#temp_acc_som_statfactureDetail.CIE_NO 
ENDIF AS cie, 
If QTE_EXP < 0 then
	'Cr$$HEX1$$e900$$ENDHEX$$dit'
else
	'D$$HEX1$$e900$$ENDHEX$$bit'
endif AS Compte,
'' as cc_date_periode,
sum(qte_exp * UPrix) AS Total
FROM ( #temp_acc_som_statfacture 
INNER JOIN ((#temp_acc_som_statfactureDetail 
LEFT JOIN t_Transport ON #temp_acc_som_statfactureDetail.PROD_NO = t_Transport.CodeTransport) 
LEFT JOIN ( t_produit 
LEFT JOIN t_verrat ON ((substring(t_produit.noproduit,5))  = (t_verrat.CodeVerrat)) and t_verrat.CodeVerrat <> '1' )
ON (#temp_acc_som_statfactureDetail.PROD_NO) = (t_produit.NoProduit)) 
ON (#temp_acc_som_statfacture.LIV_NO = #temp_acc_som_statfactureDetail.LIV_NO) AND (#temp_acc_som_statfacture.CIE_NO = #temp_acc_som_statfactureDetail.CIE_NO) ) 
INNER JOIN t_CompteAccPack 
ON (IsNull(t_Transport.NoClasse,t_produit.NoClasse) = t_CompteAccPack.NoClasse) 
AND ( (IF IsNull(t_Transport.NoClasse,t_produit.NoClasse) = 19 And actif = 0 THEN IsNull(t_verrat.CIE_NO,#temp_acc_som_statfactureDetail.CIE_NO) ELSE #temp_acc_som_statfactureDetail.CIE_NO ENDIF)  = t_CompteAccPack.NoCie)

GROUP BY
GL,
cie,
Compte,
cc_date_periode
HAVING Total <> 0"  sort="gl A cie A compte A " )
