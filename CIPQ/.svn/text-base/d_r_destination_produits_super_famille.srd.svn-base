 retrieve="SELECT 
t_Produit_Famille.SuperFamille, 
t_Produit_Famille.DescriptionSuperFamille, 
t_ELEVEUR.No_Eleveur, 
t_ELEVEUR.NOM, 
t_ELEVEUR.TELEPHONE, 
t_ELEVEUR.NB_TRUIE, 
Sum(#temp_des_pro_sufa_statfactureDetail.QTE_EXP) AS Qte 
FROM (#temp_des_pro_sufa_statfacture INNER JOIN t_ELEVEUR ON #temp_des_pro_sufa_statfacture.No_Eleveur = t_ELEVEUR.No_Eleveur) 
INNER JOIN (t_Produit_Famille INNER JOIN (#temp_des_pro_sufa_statfactureDetail 
INNER JOIN t_Produit ON #temp_des_pro_sufa_statfactureDetail.PROD_NO = t_Produit.NoProduit) 
ON t_Produit_Famille.Famille = t_Produit.Famille) ON (#temp_des_pro_sufa_statfacture.LIV_NO = #temp_des_pro_sufa_statfactureDetail.LIV_NO) 
AND (#temp_des_pro_sufa_statfacture.CIE_NO = #temp_des_pro_sufa_statfactureDetail.CIE_NO) 
GROUP BY 
t_Produit_Famille.SuperFamille, 
t_Produit_Famille.DescriptionSuperFamille, 
t_ELEVEUR.No_Eleveur, 
t_ELEVEUR.NOM, 
t_ELEVEUR.TELEPHONE, 
t_ELEVEUR.NB_TRUIE 
HAVING Qte <> 0" arguments=(("ad_de", date),("ad_au", date))  sort="superfamille A no_eleveur A " )
