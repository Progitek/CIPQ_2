 column=(type=char(16) updatewhereclause=yes name=noproduit dbname="t_statfacture.noproduit" dbalias=".noproduit" )
 column=(type=number updatewhereclause=yes name=amount dbname="amount" )
 retrieve="SELECT  
IsNull(t_Transport.NoClasse,t_produit.NoClasse) AS Classe, 
If IsNull(t_Transport.NoClasse,t_produit.NoClasse) = 19 And actif = 0 then
	IsNull(t_Verrat.CIE_NO, t_StatFactureDetail.CIE_NO)
ELSE
	t_StatFactureDetail.CIE_NO
ENDIF AS cie, 
t_StatFacture.FACT_NO, 
t_produit.NoProduit, 
qte_exp * UPrix AS Amount 
FROM t_StatFacture 
INNER JOIN (((t_StatFactureDetail 
LEFT JOIN t_produit ON (t_StatFactureDetail.PROD_NO) = (t_produit.NoProduit)) 
LEFT JOIN t_Transport ON (t_StatFactureDetail.PROD_NO) = (t_Transport.CodeTransport)) 
LEFT JOIN T_Verrat ON (substr(t_Produit.NoProduit, 5)) = (T_Verrat.CodeVerrat) ANd T_Verrat.CodeVerrat <> '1' ) 
ON (t_StatFacture.LIV_NO = t_StatFactureDetail.LIV_NO) AND (t_StatFacture.CIE_NO = t_StatFactureDetail.CIE_NO)
WHERE FACT_NO = :as_fact AND (qte_exp * UPrix )<> 0 AND Month(FACT_DATE) = Month(:ad_cur) AND Year(FACT_DATE)=  Year(:ad_cur) 
" arguments=(("ad_cur", date),("as_fact", string)) )
