 column=(type=char(100) updatewhereclause=yes name=cc_nomproduit dbname="cc_nomproduit" )
 column=(type=number updatewhereclause=yes name=totqteexp dbname="totqteexp" )
 column=(type=decimal(0) updatewhereclause=yes name=cc_classeno dbname="cc_classeno" )
 column=(type=number updatewhereclause=yes name=t_classe_semence dbname="t_classe.semence" values="	1/	0" )
