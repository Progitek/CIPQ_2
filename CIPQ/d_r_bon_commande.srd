 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"t_statfacture~" )  TABLE(NAME=~"t_statfacturedetail~" )  TABLE(NAME=~"t_eleveur~" )  TABLE(NAME=~"t_depot_spa~" ) COLUMN(NAME=~"t_statfacture.liv_date~") COLUMN(NAME=~"t_statfacture.liv_no~") COLUMN(NAME=~"t_statfacture.boncommandeclient~") COLUMN(NAME=~"t_statfacture.no_eleveur~") COLUMN(NAME=~"t_statfacture.cie_no~") COLUMN(NAME=~"t_statfacture.idtransporteur~") COLUMN(NAME=~"t_statfacture.message_liv~") COLUMN(NAME=~"t_statfacturedetail.qte_exp~") COLUMN(NAME=~"t_statfacturedetail.prod_no~") COLUMN(NAME=~"t_statfacturedetail.verrat_no~") COLUMN(NAME=~"t_statfacturedetail.description~") COLUMN(NAME=~"t_statfacturedetail.melange~") COLUMN(NAME=~"t_statfacturedetail.id_depot~") COLUMN(NAME=~"t_statfacturedetail.date_expedie_depot~") COMPUTE(NAME=~"'' as cc_client~") COLUMN(NAME=~"t_eleveur.nom~") COLUMN(NAME=~"t_eleveur.no_eleveur~") COLUMN(NAME=~"t_depot_spa.desc_depot~") COMPUTE(NAME=~"string(if exists(select 1 from t_Transport where codetransport = t_StatFactureDetail.Prod_No) then '0' else '1' endif, 1 - isnull((select t_classe.semence from t_classe inner join t_produit on t_produit.noclasse = t_classe.noclasse where t_produit.NoProduit = t_StatFactureDetail.Prod_No), 0), t_StatFactureDetail.Ligne_No) as cc_tri~")    JOIN (LEFT=~"t_statfacturedetail.id_depot~"    OP =~"=~"RIGHT=~"t_depot_spa.id_depot~"    OUTER1 =~"t_statfacturedetail.id_depot~" )    JOIN (LEFT=~"t_statfacture.cie_no~"    OP =~"=~"RIGHT=~"t_statfacturedetail.cie_no~" )    JOIN (LEFT=~"t_statfacture.no_eleveur~"    OP =~"=~"RIGHT=~"t_eleveur.no_eleveur~" )    JOIN (LEFT=~"t_statfacture.liv_no~"    OP =~"=~"RIGHT=~"t_statfacturedetail.liv_no~" )WHERE(    EXP1 =~"~~~"t_statfacture~~~".~~~"cie_no~~~"~"   OP =~"=~"    EXP2 =~":as_cie~"    LOGIC =~"And~" ) WHERE(    EXP1 =~"~~~"t_statfacture~~~".~~~"liv_no~~~"~"   OP =~"=~"    EXP2 =~":as_liv_no~" ) ) ORDER(NAME=~"19~" ASC=yes ) ARG(NAME = ~"as_cie~" TYPE = string)  ARG(NAME = ~"as_liv_no~" TYPE = string) " arguments=(("as_cie", string),("as_liv_no", string)) )
