 retrieve="SELECT 
t_Verrat.CIE_NO, 
t_Recolte.DATE_recolte, 
t_Recolte.CodeVerrat, 
t_Verrat.NOM, 
t_Verrat.TATOUAGE, 
t_Recolte.AMPO_TOTAL, 
t_CentreCIPQ.VILLE, 
1 AS Nbr,
t_Verrat.VRAIRACE,
t_Verrat.CodeRace,
t_PrepRecolte.prepnom
FROM t_Verrat 
INNER JOIN (t_CentreCIPQ 
INNER JOIN t_Recolte ON t_CentreCIPQ.CIE = t_Recolte.CIE_NO) 
ON t_Verrat.CodeVerrat = t_Recolte.CodeVerrat
INNER JOIN t_PrepRecolte ON t_Recolte.Prepose = t_PrepRecolte.prepcode
WHERE t_Recolte.DATE_recolte >= :ad_de AND t_Recolte.DATE_recolte <= :ad_au" arguments=(("ad_de", date),("ad_au", date))  sort="cie_no A date_recolte A codeverrat A " )
