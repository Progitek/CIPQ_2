 retrieve="SELECT 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.CIE_NO, 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.DateRecolte, 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.Famille, 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.NoLot, 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.CodeVerrat, 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.TATOUAGE, 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.QteDoseRecolte, 
t_AllianceMaternelle_Recolte_GestionLot_Verrat.QteDoseDistribue, 
t_AllianceMaternelle_Lot_Distribue.No_Eleveur, 
t_Eleveur.NOM, 
t_Eleveur.DroitAllianceMaternelle, 
t_AllianceMaternelle_Lot_Distribue.PROD_NO, 
t_AllianceMaternelle_Lot_Distribue.QTE_EXP,
:ad_de as ad_debut, 
:ad_a as ad_fin
FROM 
t_AllianceMaternelle_Recolte_GestionLot_Verrat 
LEFT JOIN (t_AllianceMaternelle_Lot_Distribue LEFT JOIN t_Eleveur ON t_AllianceMaternelle_Lot_Distribue.No_Eleveur = t_Eleveur.No_Eleveur) 
ON (t_AllianceMaternelle_Recolte_GestionLot_Verrat.CIE_NO = t_AllianceMaternelle_Lot_Distribue.CIE_NO) 
AND (t_AllianceMaternelle_Recolte_GestionLot_Verrat.DateRecolte = t_AllianceMaternelle_Lot_Distribue.LIV_DATE) 
AND (t_AllianceMaternelle_Recolte_GestionLot_Verrat.NoLot = t_AllianceMaternelle_Lot_Distribue.No_Lot) 
WHERE (date(t_AllianceMaternelle_Recolte_GestionLot_Verrat.DateRecolte) >= :ad_de 
AND date(t_AllianceMaternelle_Recolte_GestionLot_Verrat.DateRecolte) <= :ad_a)" update="t_AllianceMaternelle_Recolte_GestionLot_Verrat" updatewhere=1 updatekeyinplace=no arguments=(("ad_de", date),("ad_a", date))  sort="cie_no A daterecolte A nolot A codeverrat A " )
