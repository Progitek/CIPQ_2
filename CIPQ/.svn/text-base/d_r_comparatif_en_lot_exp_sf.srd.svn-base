 retrieve="SELECT
date(#temp_com_en_lot_statfacture.LIV_DATE) as liv_date, 
If VERRAT_NO is not null then
	'Pure'
else
	Famille
endif AS FamilleProduit, 
Sum(#temp_com_en_lot_statfactureDetail.QTE_EXP) AS QteExpediee 
FROM #temp_com_en_lot_statfacture 
INNER JOIN (#temp_com_en_lot_statfactureDetail 
LEFT JOIN t_Produit ON #temp_com_en_lot_statfactureDetail.PROD_NO = t_Produit.NoProduit) 
ON (#temp_com_en_lot_statfacture.LIV_NO = #temp_com_en_lot_statfactureDetail.LIV_NO) 
AND (#temp_com_en_lot_statfacture.CIE_NO = #temp_com_en_lot_statfactureDetail.CIE_NO) 
WHERE date(#temp_com_en_lot_statfacture.LIV_DATE) = date(:adt_cur) AND FamilleProduit = :as_famille
GROUP BY date(#temp_com_en_lot_statfacture.LIV_DATE), FamilleProduit
HAVING FamilleProduit is not null AND QteExpediee <> 0" arguments=(("adt_cur", datetime),("as_famille", string),("al_qtemiseenlot", number)) )
