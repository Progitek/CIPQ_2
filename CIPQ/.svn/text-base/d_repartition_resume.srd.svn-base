 column=(type=char(16) updatewhereclause=yes name=t_commandedetail_noproduit dbname="t_commandedetail.noproduit" dbalias=".noproduit" )
 column=(type=char(31) updatewhereclause=yes name=noproduit2 dbname="noproduit2" )
 column=(type=char(1) updatewhereclause=yes name=t_commandedetail_centre dbname="t_commandedetail.centre" dbalias=".centre" )
 retrieve="SELECT 
Sum(t_CommandeDetail.QteInit) AS QInit, 
Sum(t_CommandeDetail.QteCommande) AS QComm, 
Sum(t_CommandeDetail.QteExpedie) AS QExp, 
Sum(t_CommandeDetail.QteTransfert) AS QTrans, 
t_CommandeDetail.NoProduit, 
upper(if t_CommandeDetail.CodeVerrat is null OR t_CommandeDetail.CodeVerrat = '' then
	t_CommandeDetail.NoProduit
else
	t_CommandeDetail.CodeVerrat + ' (' + t_CommandeDetail.NoProduit + ')'
endif)
as NoProduit2,
t_CommandeDetail.Trans AS Centre
FROM t_Commande 
INNER JOIN t_CommandeDetail ON (t_Commande.NoCommande = t_CommandeDetail.NoCommande) 
AND (t_Commande.CieNo = t_CommandeDetail.CieNo) 
GROUP BY t_CommandeDetail.NoProduit, 
NoProduit2, 
t_CommandeDetail.Trans, 
t_Commande.DateCommande 
HAVING t_Commande.DateCommande=:ad_datecommande AND upper(NoProduit2) = upper(:as_produit2)" arguments=(("ad_datecommande", date),("as_produit2", string)) )
