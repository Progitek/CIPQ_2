 retrieve="SELECT 
t_Recolte_GestionLot_Detail.DateRecolte, 
t_Recolte_GestionLot_Famille.Type, 
t_Recolte_GestionLot_Famille.Famille, 
t_Recolte_GestionLot_Detail.NoLot, 
t_Recolte_GestionLot_Detail.CodeVerrat, 
QteDoseMelange,
QteDosePure ,
(If t_Recolte_GestionLot_Famille.Type = 1 THEN QteDoseMelange else QteDosePure endif) AS QteDose,
t_recolte.heure_recolte
FROM 
t_Recolte_GestionLot_Famille 
INNER JOIN (t_Recolte_GestionLot_Lot INNER JOIN t_Recolte_GestionLot_Detail ON 
(t_Recolte_GestionLot_Lot.NoLot = t_Recolte_GestionLot_Detail.NoLot) AND (t_Recolte_GestionLot_Lot.Famille = t_Recolte_GestionLot_Detail.Famille) AND 
(t_Recolte_GestionLot_Lot.DateRecolte = t_Recolte_GestionLot_Detail.DateRecolte)) ON (t_Recolte_GestionLot_Famille.Famille = t_Recolte_GestionLot_Lot.Famille) AND 
(t_Recolte_GestionLot_Famille.DateRecolte = t_Recolte_GestionLot_Lot.DateRecolte)
INNER JOIN t_recolte	ON t_Recolte_GestionLot_Detail.codeverrat = t_recolte.codeverrat 
AND t_Recolte_GestionLot_Detail.daterecolte = t_recolte.date_recolte 
WHERE
t_Recolte_GestionLot_Famille.DateRecolte = :ad_cur AND
t_Recolte_GestionLot_Famille.Famille = :as_famille AND
t_Recolte_GestionLot_Lot.NoLot = :as_lot
ORDER BY 
t_Recolte_GestionLot_Famille.Type, 
t_Recolte_GestionLot_Famille.Famille, 
t_Recolte_GestionLot_Detail.NoLot, 
t_Recolte_GestionLot_Detail.CodeVerrat" arguments=(("ad_cur", date),("as_famille", string),("as_lot", string)) )
