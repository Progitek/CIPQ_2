 retrieve="SELECT t_RECOLTE.CodeVerrat, 
t_RECOLTE.AMPO_TOTAL AS NbDoses, 

isnull((SELECT sum(QteDistribue) 
FROM t_Recolte_GestionLot_Produit_QteDistribue, t_Recolte_GestionLot_Produit 
WHERE date(t_Recolte_GestionLot_Produit.DateRecolte) = Date(today()) 
And t_Recolte_GestionLot_Produit.CodeVerrat=t_recolte.CodeVerrat AND
t_Recolte_GestionLot_Produit.daterecolte = t_Recolte_GestionLot_Produit.daterecolte AND
upper(t_Recolte_GestionLot_Produit.famille) = upper(t_Recolte_GestionLot_Produit.famille) AND
t_Recolte_GestionLot_Produit.nolot = t_Recolte_GestionLot_Produit.nolot AND
upper(t_Recolte_GestionLot_Produit.noproduit) = upper(t_Recolte_GestionLot_Produit.noproduit) AND
t_Recolte_GestionLot_Produit.compteur = t_Recolte_GestionLot_Produit.compteur
),0) AS cc_QteDistribue, 

NbDoses - cc_QteDistribue AS Disponible 
FROM t_RECOLTE 
INNER JOIN t_Verrat_Classe ON t_RECOLTE.Classe = t_Verrat_Classe.ClasseVerrat 
INNER JOIN t_Produit ON upper(t_Verrat_Classe.Famille) = upper(t_Produit.Famille )
WHERE 
t_RECOLTE.AMPO_TOTAL <> 0 AND 
date(t_RECOLTE.DATE_recolte) = date(today()) AND 
upper(t_Produit.NoProduit) = upper(:as_produit)
ORDER BY t_RECOLTE.Heure_Recolte" arguments=(("as_produit", string)) )
