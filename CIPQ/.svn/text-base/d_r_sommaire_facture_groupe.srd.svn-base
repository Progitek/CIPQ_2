 retrieve="SELECT 
qryFactSom.No_Eleveur, 
qryFactSom.FACT_NO, 
qryFactSom.SommeDeVENTE, 
t_ELEVEUR.taxe_fede, 
t_ELEVEUR.taxe_prov,
qryFactSom.pourc_fed,
qryFactSom.pourc_prov,
round(SommeDeTAXEF ,2) AS TPS, 
round(SommeDeTAXEP ,2) AS TVQ, 
SommeDeVENTE + tps + tvq AS Total, 
t_ELEVEUR_Group.IDGroup, 
t_ELEVEUR_Group.Description, 
t_ELEVEUR_GroupSecondaire.idgroupsecondaire,
t_ELEVEUR_GroupSecondaire.NomGroupSecondaire, 
t_ELEVEUR.NOM, 
t_ELEVEUR.ADRESSE, 
t_ELEVEUR.RUE, 
t_ELEVEUR.VILLE, 
t_ELEVEUR.CONTE, 
t_ELEVEUR.Province, 
t_ELEVEUR.REG_AGR, 
t_ELEVEUR.CODE_POST, 
t_ELEVEUR.liv_adr_a, 
t_ELEVEUR.liv_vil_a, 
t_ELEVEUR.liv_cod_a, 
t_ELEVEUR.liv_tel_a, 
t_ELEVEUR.liv_conte, 
t_ELEVEUR.liv_notran, 
t_ELEVEUR.Secteur_transporteur, 
t_ELEVEUR.TELEPHONE, 
t_ELEVEUR.Billing_cycle_code 
FROM t_ELEVEUR_Group 
INNER JOIN (t_ELEVEUR_GroupSecondaire 
RIGHT JOIN (t_ELEVEUR INNER JOIN 

(SELECT 
#Temp_som_fact_grp_StatFacture.No_Eleveur, 
#Temp_som_fact_grp_StatFacture.FACT_NO, 
Sum(isnull(VENTE,0)) AS SommeDeVENTE, 
Sum(round(#Temp_som_fact_grp_StatFacture.TAXEP,2)) AS SommeDeTAXEP, 
Sum(round(#Temp_som_fact_grp_StatFacture.TAXEF,2)) AS SommeDeTAXEF, 
#Temp_som_fact_grp_StatFacture.Pourc_Fed, 
#Temp_som_fact_grp_StatFacture.Pourc_Prov
FROM #Temp_som_fact_grp_StatFacture
GROUP BY 
#Temp_som_fact_grp_StatFacture.No_Eleveur, 
#Temp_som_fact_grp_StatFacture.FACT_NO, 
#Temp_som_fact_grp_StatFacture.Pourc_Fed, 
#Temp_som_fact_grp_StatFacture.Pourc_Prov)

as qryFactSom ON t_ELEVEUR.No_Eleveur = qryFactSom.No_Eleveur) 
ON t_ELEVEUR_GroupSecondaire.IDGroupSecondaire = t_ELEVEUR.GroupeSecondaire) 
ON t_ELEVEUR_Group.IDGroup = t_ELEVEUR.Groupe 
WHERE qryFactSom.SommeDeVENTE <> 0
