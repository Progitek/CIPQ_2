 retrieve="SELECT 
t_RECOLTE.DATE_recolte, 
t_RECOLTE.CodeVerrat, 
t_RECOLTE.Classe, 
t_RECOLTE.Exclusif, 
t_RECOLTE.Gedis, 
t_RECOLTE.AMPO_TOTAL AS QteRecolte, 

(SELECT Sum(t_Recolte_GestionLot_Detail.QteDosePure) AS Pure 
FROM t_Recolte_GestionLot_Detail
WHERE date(t_Recolte_GestionLot_Detail.DateRecolte) = date(t_RECOLTE.DATE_recolte)
AND upper(t_Recolte_GestionLot_Detail.CodeVerrat) = upper(t_RECOLTE.CodeVerrat)
) as Pure,

(SELECT Sum(t_Recolte_GestionLot_Detail.QteDoseMelange) AS Melange
FROM t_Recolte_GestionLot_Detail
WHERE date(t_Recolte_GestionLot_Detail.DateRecolte) = date(t_RECOLTE.DATE_recolte) 
AND upper(t_Recolte_GestionLot_Detail.CodeVerrat) = upper(t_RECOLTE.CodeVerrat)
) as Melange,

AMPO_TOTAL - (isnull(Pure,0) + isnull(Melange,0)) AS Qtedisponible 
FROM t_RECOLTE 
WHERE date(t_RECOLTE.DATE_recolte) = Date(:ad_date) AND 
(isnull(AMPO_TOTAL,0) - (isnull(Pure,0) + isnull(Melange,0)) <> 0)
ORDER BY t_RECOLTE.CodeVerrat" arguments=(("ad_date", date)) )
