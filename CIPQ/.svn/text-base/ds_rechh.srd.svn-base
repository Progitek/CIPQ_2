 retrieve="SELECT DISTINCT
t_StatFacture.Accpac_Date,
t_StatFacture.No_Eleveur,
t_ELEVEUR.Groupe,
t_ELEVEUR_Group.Description,
t_ELEVEUR_Group.TypePayeur,
t_ELEVEUR_GroupSecondaire.NomGroupSecondaire,
t_ELEVEUR_GroupSecondaire.NonPayeur,
t_ELEVEUR.Billing_cycle_code,
Sum(Round(isnull(VENTE,0),2)) AS SumOfVENTE,
t_StatFacture.Pourc_Fed,
t_StatFacture.Pourc_Prov,
Round(SumOfVENTE * (Max(t_StatFacture.Pourc_Fed) / 100),2) AS TPS,
Round((SumOfVENTE + TPS) * (Max(t_StatFacture.Pourc_Prov) / 100),2) AS TVQ,
t_StatFacture.FACT_DATE, 
t_StatFacture.FACT_NO 
FROM t_ELEVEUR_Group 
RIGHT JOIN (t_ELEVEUR_GroupSecondaire 
RIGHT JOIN (t_ELEVEUR 
INNER JOIN t_StatFacture ON t_ELEVEUR.No_Eleveur = t_StatFacture.No_Eleveur) 
ON t_ELEVEUR_GroupSecondaire.IDGroupSecondaire = t_ELEVEUR.GroupeSecondaire) 
ON t_ELEVEUR_Group.IDGroup = t_ELEVEUR.Groupe 
WHERE Month(FACT_DATE) = month(:ad_cur) And Year(FACT_DATE) = year(:ad_cur )
GROUP BY
t_StatFacture.Accpac_Date,
t_StatFacture.No_Eleveur,
t_ELEVEUR.Groupe,
t_ELEVEUR_Group.Description,
t_ELEVEUR_Group.TypePayeur,
t_ELEVEUR_GroupSecondaire.NomGroupSecondaire,
t_ELEVEUR_GroupSecondaire.NonPayeur,
t_ELEVEUR.Billing_cycle_code,
t_StatFacture.Pourc_Fed,
t_StatFacture.Pourc_Prov,
t_StatFacture.FACT_DATE,
t_StatFacture.FACT_NO
HAVING (Not (t_StatFacture.FACT_NO) Is Null) And (Sum(isnull(t_StatFacture.VENTE,0)) <> 0)
ORDER BY t_StatFacture.FACT_NO" arguments=(("ad_cur", date)) )
