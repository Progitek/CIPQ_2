 column=(type=char(3) updatewhereclause=yes name=cie dbname="cie" )
 retrieve="SELECT DISTINCT 
IsNull(t_Transport.NoClasse,t_Produit.NoClasse) AS Classe, 
If IsNull(t_Transport.NoClasse,t_Produit.NoClasse) = 19 And actif=0 then
	IsNull(t_Verrat.CIE_NO,t_StatFactureDetail.CIE_NO)
ELSE
	t_StatFactureDetail.CIE_NO
ENDIF AS cie 
FROM (((t_StatFacture INNER JOIN t_StatFactureDetail ON (t_StatFacture.LIV_NO = t_StatFactureDetail.LIV_NO) AND (t_StatFacture.CIE_NO = t_StatFactureDetail.CIE_NO)) 
LEFT JOIN t_Produit ON t_StatFactureDetail.PROD_NO = t_Produit.NoProduit) 
LEFT JOIN t_Transport ON t_StatFactureDetail.PROD_NO = t_Transport.CodeTransport) 
LEFT JOIN t_Verrat ON (substr(t_Produit.NoProduit, 5)) = (t_Verrat.CodeVerrat ) AND t_Verrat.CodeVerrat <> '1'
WHERE (QTE_EXP * UPRIX) <> 0 AND Month(FACT_DATE)= Month(:ad_date) AND Year(FACT_DATE) = Year(:ad_date) AND cie is not null 
AND IsNull(t_Transport.NoClasse,t_Produit.NoClasse) is not null 
order by classe

" arguments=(("ad_date", date)) )
