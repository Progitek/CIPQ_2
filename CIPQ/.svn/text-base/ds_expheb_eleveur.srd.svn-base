 column=(type=number update=yes updatewhereclause=yes name=totalexped dbname="t_eleveur.totalexped" )
 retrieve="  SELECT t_Produit.CodeHebergeur AS Code_Hebergeur,
			t_ELEVEUR.No_Eleveur,
			t_Eleveur.NOM,
			t_Eleveur.ADRESSE,
			t_Eleveur.RUE,
			t_Eleveur.VILLE,
			t_Eleveur.CONTE,
			t_Eleveur.CODE_POST,
			date(T_StatFacture.LIV_DATE) as FACT_LIV_DATE,
			T_StatFactureDetail.PROD_NO,
			Sum(T_StatFactureDetail.QTE_EXP) AS TotalExped
	 FROM T_StatFacture INNER JOIN T_StatFactureDetail ON T_StatFactureDetail.CIE_NO = T_StatFacture.CIE_NO
																		AND T_StatFactureDetail.LIV_NO = T_StatFacture.LIV_NO
							  INNER JOIN t_Eleveur ON T_StatFacture.No_Eleveur = t_Eleveur.No_Eleveur
							  INNER JOIN t_Produit ON t_Produit.NoProduit = T_StatFactureDetail.PROD_NO
	WHERE date(T_StatFacture.LIV_DATE) >= :ad_de
	  and date(T_StatFacture.LIV_DATE) <= :ad_a
	  and t_Produit.NoClasse = 9
	  and T_StatFactureDetail.PROD_NO Not Like 'PUR%'
GROUP BY Code_Hebergeur,
			t_ELEVEUR.No_Eleveur,
			t_Eleveur.NOM,
			t_Eleveur.ADRESSE,
			t_Eleveur.RUE,
			t_Eleveur.VILLE,
			t_Eleveur.CONTE,
			t_Eleveur.CODE_POST,
			FACT_LIV_DATE,
			T_StatFactureDetail.PROD_NO
  HAVING TotalExped <> 0
UNION ALL
  SELECT Left(T_Verrat.CodeVerrat,1) AS Code_Hebergeur,
			t_ELEVEUR.No_Eleveur,
			t_Eleveur.NOM,
			t_Eleveur.ADRESSE,
			t_Eleveur.RUE,
			t_Eleveur.VILLE,
			t_Eleveur.CONTE,
			t_Eleveur.CODE_POST,
			date(T_StatFacture.LIV_DATE) as FACT_LIV_DATE,
			T_StatFactureDetail.VERRAT_NO AS PROD_NO,
			Sum(T_StatFactureDetail.QTE_EXP) AS TotalExped
	 FROM T_StatFacture INNER JOIN T_StatFactureDetail ON T_StatFactureDetail.CIE_NO = T_StatFacture.CIE_NO
																		AND T_StatFactureDetail.LIV_NO = T_StatFacture.LIV_NO
							  INNER JOIN t_Eleveur ON T_StatFacture.No_Eleveur = t_Eleveur.No_Eleveur
							  INNER JOIN T_Verrat ON T_Verrat.CodeVerrat = T_StatFactureDetail.VERRAT_NO
	WHERE date(T_StatFacture.LIV_DATE) >= :ad_de
	  and date(T_StatFacture.LIV_DATE) <= :ad_a
GROUP BY Code_Hebergeur,
			t_ELEVEUR.No_Eleveur,
			t_Eleveur.NOM,
			t_Eleveur.ADRESSE,
			t_Eleveur.RUE,
			t_Eleveur.VILLE,
			t_Eleveur.CONTE,
			t_Eleveur.CODE_POST,
			FACT_LIV_DATE,
			PROD_NO
  HAVING TotalExped <> 0
ORDER BY Code_Hebergeur" update="t_Eleveur" updatewhere=1 updatekeyinplace=no arguments=(("ad_de", date),("ad_a", date)) )
