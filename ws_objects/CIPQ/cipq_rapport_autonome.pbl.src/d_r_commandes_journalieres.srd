 retrieve="SELECT
#temp_com_jou_statfacture.CIE_NO,
#temp_com_jou_statfactureDetail.PROD_NO,
IsNull(t_Produit.NomProduit,#temp_com_jou_statfactureDetail.Description) AS cc_NomProduit,
isnull(Sum(#temp_com_jou_statfactureDetail.QTE_EXP),0) AS totQteExp,
IsNull(t_Produit.NoClasse,t_Transport.NoClasse) AS cc_ClasseNo,
t_Classe.Semence,
if right(#temp_com_jou_statfactureDetail.PROD_NO,3) = '-GS' then
	1
else
	0
endif AS GEDIS
FROM 
#temp_com_jou_statfacture 
INNER JOIN ((#temp_com_jou_statfactureDetail 
LEFT JOIN t_Produit ON #temp_com_jou_statfactureDetail.PROD_NO = t_Produit.NoProduit) 
LEFT JOIN t_Transport ON #temp_com_jou_statfactureDetail.PROD_NO = t_Transport.CodeTransport) 
ON (#temp_com_jou_statfacture.LIV_NO = #temp_com_jou_statfactureDetail.LIV_NO) 
AND (#temp_com_jou_statfacture.CIE_NO = #temp_com_jou_statfactureDetail.CIE_NO) 
INNER JOIN t_Classe ON IsNull(t_Produit.NoClasse,t_Transport.NoClasse) = t_Classe.NoClasse 

WHERE 
(Lower(#temp_com_jou_statfactureDetail.PROD_NO) In ('fcse','fcseg','fmat','fgold','fgold100','0080','0081','0100','lmai','501','502','503','504','505','506','507','508','509','518','212','519','0519','0212','0080-A','FCSE-A','FCSE-B')
OR t_Classe.Semence <> 0
OR t_Classe.NoClasse = 6)

GROUP BY #temp_com_jou_statfacture.CIE_NO, 
#temp_com_jou_statfactureDetail.PROD_NO, 
cc_NomProduit, 
cc_ClasseNo ,
t_Classe.Semence

HAVING totQteExp <> 0" arguments=(("ad_de", date))  sort="cie_no A t_classe_semence D prod_no A cc_nomproduit A " )
