 retrieve="SELECT 
t_ELEVEUR_Group.IDGroup, 
t_ELEVEUR_Group.Description , 
t_ELEVEUR_GroupSecondaire.IDGroupSecondaire, 
t_ELEVEUR_GroupSecondaire.NomGroupSecondaire, 
t_ELEVEUR_Group.Code_Hebergeur, 
t_statfacture.FACT_NO, 
t_ELEVEUR.No_Eleveur, 
t_ELEVEUR.NOM, 
t_ELEVEUR.Secteur_transporteur, 
t_ELEVEUR.taxe_prov, 
t_ELEVEUR.taxe_fede, 
t_ELEVEUR.Adresse,
t_ELEVEUR.Rue,
t_ELEVEUR.Ville,
t_ELEVEUR.Conte,
t_ELEVEUR.Code_post,
t_statfactureDetail.PROD_NO, 
t_statfactureDetail.VERRAT_NO, 
Sum(t_statfactureDetail.QTE_EXP) AS SommeDeQTE_EXP, 
t_statfactureDetail.UPRIX,
t_ELEVEUR.Billing_cycle_code,
:al_new as new_page_flag,
t_statfacture.pourc_fed,
t_statfacture.pourc_prov,
t_statfacture.vente,
t_statfacture.taxep,
t_statfacture.taxef,
isnull((IF t_statfactureDetail.PROD_NO IS NULL AND t_statfactureDetail.VERRAT_NO IS NULL THEN
    ''
 ELSE
    IF (SELECT t_produit.noclasse FROM t_produit WHERE t_produit.noProduit = t_statfactureDetail.PROD_NO) = 9 THEN
        (SELECT t_produit.type_emballage FROM t_produit WHERE t_produit.Noproduit = t_statfactureDetail.PROD_NO)
    ELSE
        t_statfactureDetail.PROD_NO
    ENDIF
ENDIF),t_statfactureDetail.PROD_NO) as embal,

isnull((SELECT FIRST t_transport.NOM FROM t_transport WHERE t_transport.CodeTransport = t_statfactureDetail.PROD_NO),
    IF (SELECT t_produit.noclasse FROM t_produit WHERE t_produit.noProduit = t_statfactureDetail.PROD_NO) = 9 THEN 
        '' 
    ELSE
        (SELECT FIRST t_produit.nomproduit FROM t_produit WHERE t_produit.NoProduit = t_statfactureDetail.PROD_NO)
    ENDIF) as prod,
	 t_statfacturedetail.tps,
	 t_statfacturedetail.tvq
FROM 
(t_ELEVEUR_Group 
RIGHT JOIN ((t_ELEVEUR_GroupSecondaire 
RIGHT JOIN t_ELEVEUR ON (t_ELEVEUR_GroupSecondaire.IDGroup = t_ELEVEUR.Groupe) AND (t_ELEVEUR_GroupSecondaire.IDGroupSecondaire = t_ELEVEUR.GroupeSecondaire)) 
INNER JOIN t_statfacture ON t_ELEVEUR.No_Eleveur = t_statfacture.No_Eleveur) ON t_ELEVEUR_Group.IDGroup = t_ELEVEUR.Groupe) 
INNER JOIN t_statfactureDetail ON (t_statfacture.LIV_NO = t_statfactureDetail.LIV_NO) AND (t_statfacture.CIE_NO = t_statfactureDetail.CIE_NO)
WHERE t_statfactureDetail.UPRIX <> 0 AND t_statfacture.fact_date = :ad_de
GROUP BY 
t_ELEVEUR_Group.IDGroup, 
t_ELEVEUR_Group.Description, 
t_ELEVEUR_GroupSecondaire.IDGroupSecondaire, 
t_ELEVEUR_GroupSecondaire.NomGroupSecondaire, 
t_ELEVEUR_Group.Code_Hebergeur, 
t_statfacture.FACT_NO,
embal,
prod,
t_ELEVEUR.No_Eleveur, 
t_ELEVEUR.Secteur_transporteur, 
t_ELEVEUR.taxe_prov, 
t_ELEVEUR.taxe_fede, 
t_ELEVEUR.NOM,
t_statfactureDetail.PROD_NO, 
t_statfactureDetail.VERRAT_NO, 
t_statfactureDetail.UPRIX,
t_ELEVEUR.Billing_cycle_code,
t_ELEVEUR.Adresse,
t_ELEVEUR.Rue,
t_ELEVEUR.Ville,
t_ELEVEUR.Conte,
t_ELEVEUR.Code_post,
t_statfacture.pourc_fed,
t_statfacture.pourc_prov,
t_statfacture.vente,
t_statfacture.taxep,
t_statfacture.taxef,
t_statfactureDetail.liv_no,
t_StatFacturedetail.ligne_no,
t_statfacturedetail.tps,
t_statfacturedetail.tvq
HAVING SommeDeQTE_EXP <> 0
ORDER BY embal DESC,
         prod_no ASC,
         idgroupsecondaire ASC,
         t_ELEVEUR.No_Eleveur ASC , 
         t_statfacture.FACT_NO ASC,
         uprix ASC
" arguments=(("ad_de", date),("al_new", number))  sort="description A nomgroupsecondaire A no_eleveur A fact_no A embal A " )
