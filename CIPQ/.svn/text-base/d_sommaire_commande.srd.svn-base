 retrieve="SELECT t_Commande.No_Eleveur, 
		t_Commande.DateCommande, 
		t_CommandeDetail.TranName,
		t_CommandeDetail.nocommande, 
		t_CommandeDetail.norepeat, 
		t_CommandeDetail.QteCommande, 
		t_CommandeDetail.noproduit,
		t_CommandeDetail.codeverrat,
		t_CommandeDetail.CieNo,
		t_CommandeDetail.NoLigne,
		t_CommandeDetail.description,
		t_CommandeDetail.qteinit,
		t_CommandeDetail.compteur,
		t_Commande.transferepar,
		t_produit.nomproduit
FROM t_Commande 
INNER JOIN t_CommandeDetail 
ON (t_Commande.NoCommande = t_CommandeDetail.NoCommande) AND (t_Commande.CieNo = t_CommandeDetail.CieNo) 
LEFT JOIN t_produit ON upper(t_CommandeDetail.noproduit) = upper(t_produit.noproduit)
WHERE t_Commande.No_Eleveur = :al_eleveur AND (isnull(t_CommandeDetail.QteCommande,0 ) - isnull(t_CommandeDetail.qtetransfert,0 )) > 0
ORDER BY t_Commande.DateCommande, t_CommandeDetail.NoCommande, t_CommandeDetail.NoLigne" update="t_CommandeDetail" updatewhere=0 updatekeyinplace=no arguments=(("al_eleveur", number)) )
