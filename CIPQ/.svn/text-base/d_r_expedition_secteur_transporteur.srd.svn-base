 retrieve="SELECT #temp_exp_sec_tra_statfacture.CIE_NO, 
t_CentreCIPQ.VILLE,
#temp_exp_sec_tra_statfacture.IDTransporteur, 
t_Transporteur.Secteur,
#temp_exp_sec_tra_statfactureDetail.PROD_NO, 
t_Produit.NomProduit,
isnull(Sum(#temp_exp_sec_tra_statfactureDetail.QTE_EXP),0) AS SommeDeQTE_EXP 
FROM ((#temp_exp_sec_tra_statfacture 
INNER JOIN t_CentreCIPQ ON #temp_exp_sec_tra_statfacture.CIE_NO = t_CentreCIPQ.CIE) 
INNER JOIN t_Transporteur ON #temp_exp_sec_tra_statfacture.IDTransporteur = t_Transporteur.IDTransporteur) 
INNER JOIN (#temp_exp_sec_tra_statfactureDetail 
INNER JOIN t_Produit ON #temp_exp_sec_tra_statfactureDetail.PROD_NO = t_Produit.NoProduit) 
ON (#temp_exp_sec_tra_statfacture.LIV_NO = #temp_exp_sec_tra_statfactureDetail.LIV_NO) 
AND (#temp_exp_sec_tra_statfacture.CIE_NO = #temp_exp_sec_tra_statfactureDetail.CIE_NO) 
WHERE t_Produit.Special= 1 
GROUP BY 
#temp_exp_sec_tra_statfacture.CIE_NO,
t_CentreCIPQ.VILLE, 
#temp_exp_sec_tra_statfacture.IDTransporteur,
t_Transporteur.Secteur, 
#temp_exp_sec_tra_statfactureDetail.PROD_NO,
t_Produit.NomProduit 
HAVING SommeDeQTE_EXP <> 0" arguments=(("ad_de", date),("ad_au", date))  sort="idtransporteur A cie_no A prod_no A " )
