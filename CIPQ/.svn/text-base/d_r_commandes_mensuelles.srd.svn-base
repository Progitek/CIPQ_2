 retrieve="SELECT 
date(#temp_com_men_statfacture.LIV_DATE) as liv_date,
#temp_com_men_statfacture.CIE_NO, 
Sum(#temp_com_men_statfactureDetail.QTE_EXP) AS SumOfQTE_EXP,
t_centrecipq.ville
FROM #temp_com_men_statfacture 
INNER JOIN (#temp_com_men_statfactureDetail 
INNER JOIN t_Produit ON #temp_com_men_statfactureDetail.PROD_NO = t_Produit.NoProduit) 
ON (#temp_com_men_statfacture.LIV_NO = #temp_com_men_statfactureDetail.LIV_NO) 
AND (#temp_com_men_statfacture.CIE_NO = #temp_com_men_statfactureDetail.CIE_NO) 
INNER JOIN t_centrecipq ON  t_centrecipq.CIE = #temp_com_men_statfacture.CIE_NO
WHERE t_Produit.Special = 1
GROUP BY 
date(#temp_com_men_statfacture.LIV_DATE),
#temp_com_men_statfacture.CIE_NO,
t_centrecipq.ville" arguments=(("ad_de", date))  sort="cie_no A liv_date A " )
